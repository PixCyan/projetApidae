<div class="sidebar-module">
    <h6 class="sidebar-title"> {{ 'Services'|trans }}</h6>
    {% for s in services %}
        <div class="sidebar-module-inner">
            <div class="checkbox-block">
                <input id="{{ s.serId }}" name="services" value="{{ s.serId }}" type="checkbox" class="filtres"/>
                <label class="" for="{{ s.serId }}">{{ s.serLibelle|langueLib(langue.lanShortCut) }}</label>
            </div>
        </div>
    {% endfor %}
</div>
