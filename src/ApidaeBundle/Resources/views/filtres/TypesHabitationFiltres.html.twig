<div class="sidebar-module">
    <h6 class="sidebar-title"> {{ "Types d'habitation"|trans }} <span id="titreTypeHabitation" >{% if countCategories is defined %}({{ countCategories }}){% endif %}</span></h6>
    {% for t in typesHabitation %}
        <div class="sidebar-module-inner">
            <div class="checkbox-block">
                <input id="{{ t.catId }}" name="categories" value="{{ t.catId }}" type="checkbox" class="filtres"/>
                <label class="" for="{{ t.catId }}">{{ t.catLibelle|langueLib(langue.lanShortCut) }}</label>
            </div>
        </div>
    {% endfor %}
</div>
