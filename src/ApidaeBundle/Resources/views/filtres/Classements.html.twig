<div class="sidebar-module">
    <h6 class="sidebar-title">{{ 'Classements'|trans }} </h6>
    {% for l in labels %}
        <div class="sidebar-module-inner">
            <div class="checkbox-block">
                <input id="{{ l.labId }}" name="services" value="{{ l.labId }}" type="checkbox" class="filtres"/>
                <label class="" for="{{ l.labId }}">{{ l.labLibelle|langueLib(langue.lanShortCut) }} : {{ l.labClassement|langueLib(langue.lanShortCut) }}</label>
            </div>
        </div>

