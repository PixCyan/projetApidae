<div class="sidebar-module">
    <h6 class="sidebar-title">{{ 'Classements'|trans }} <span id="titreClassement">{% if countClassements is defined %}({{  countClassements }}){% endif %}</span></h6>
    {% for l in labels %}
        <div class="sidebar-module-inner">
            <div class="checkbox-block">
                <input id="{{ l.labId }}" name="classements" value="{{ l.labId }}" type="checkbox" class="filtres"/>
                <label class="" for="{{ l.labId }}">{{ l.labLibelle|langueLib(langue.lanShortCut) }} : {{ l.labClassement|langueLib(langue.lanShortCut) }}</label>
            </div>
        </div>

        <!-- labels qualite -->
    {% endfor %}
</div>
